{
    "name": "red1986/laravel-telegram-api",
    "description": "Laravel 8 api for using telegram bot",
    "homepage": "https://github.com/RED1986/laravel-telegram-api",
    "keywords": ["Laravel", "telegram"],
    "require": {
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelTelegramApi\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelTelegramApi\\LaravelTelegramApiServiceProvider"
            ]
        }
    }
}
